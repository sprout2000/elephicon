{
  "name": "elephicon",
  "productName": "Elephicon",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "version": "2.5.3",
  "license": "MIT",
  "author": {
    "name": "sprout2000"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "package": "cross-env NODE_ENV=\"development\" ts-node scripts/builder.ts",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/main.js ./dist/index.html && electronmon .",
    "release": "ts-node scripts/builder.ts"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.3",
    "i18next": "^23.2.11",
    "localforage": "^1.10.0",
    "mime-types": "^2.1.35",
    "png2icons": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.16.19",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "electron": "^25.3.0",
    "electron-builder": "^24.5.2",
    "electronmon": "^2.0.2",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "semver": "^7.5.2"
  }
}
