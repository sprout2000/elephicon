{
  "name": "elephicon",
  "productName": "Elephicon",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "version": "3.8.2",
  "license": "MIT",
  "author": {
    "name": "sprout2000"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:rspack dev:electron",
    "dev:rspack": "rspack --node-env \"development\"",
    "dev:electron": "wait-on ./dist/index.html ./dist/main.js && electronmon .",
    "build": "rimraf dist && rspack --node-env \"production\"",
    "lint": "biome check --write .",
    "release": "ts-node scripts/builder.ts"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.6.8",
    "i18next": "^25.5.2",
    "localforage": "^1.10.0",
    "mime-types": "^3.0.1",
    "png2icons": "^2.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@rspack/cli": "^1.5.4",
    "@rspack/core": "^1.5.4",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.18.5",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.2",
    "electron": "^38.1.1",
    "electron-builder": "^24.13.3",
    "electronmon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "sass": "^1.92.1",
    "sass-loader": "^16.0.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "wait-on": "^8.0.5"
  }
}
