{
  "name": "elephicon",
  "productName": "Elephicon",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "version": "2.3.1",
  "license": "MIT",
  "author": {
    "name": "sprout2000"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:*",
    "build": "cross-env NODE_ENV=\"production\" webpack --progress",
    "package": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=\"false\" ts-node scripts/build.ts",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/main.js ./dist/index.html && electronmon .",
    "release": "ts-node scripts/build.ts"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "i18next": "^22.4.9",
    "localforage": "^1.10.0",
    "mime-types": "^2.1.35",
    "png2icons": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "electron": "^22.3.0",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "sass": "^1.58.1",
    "sass-loader": "^13.2.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "overrides": {
    "minimatch": "^3.0.5"
  },
  "volta": {
    "node": "16.19.1"
  }
}
