{
  "name": "elephicon",
  "productName": "Elephicon",
  "version": "1.1.0",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "author": "sprout2000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/elephicon/issues"
  },
  "keywords": [
    "electron",
    "ico",
    "icns",
    "png2icons",
    "cross-platform"
  ],
  "engines": {
    "node": ">=12.13"
  },
  "main": "dist/main.js",
  "scripts": {
    "start": "run-s clean build serve",
    "predev": "rimraf dist",
    "dev": "run-p dev:*",
    "dev:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=\"development\" electron .",
    "dev:tsc": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "webpack --watch",
    "serve": "electron .",
    "clean": "rimraf dist release",
    "build": "webpack --config webpack.config.prod.ts --progress",
    "build:unsign": "ts-node scripts/build-unsign.ts",
    "build:darwin": "ts-node scripts/build.ts",
    "build:appx": "ts-node scripts/build-appx.ts",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx --fix",
    "lint:prettier": "prettier --write .",
    "package": "run-s clean build build:unsign",
    "package:appx": "run-s clean build build:appx",
    "package:darwin": "run-s clean build build:darwin",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "dependencies": {
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "i18next": "^20.6.1",
    "mime-types": "^2.1.32",
    "png2icons": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "typeface-roboto": "^1.1.13",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/mini-css-extract-plugin": "^2.2.0",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/ua-parser-js": "^0.7.36",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "dotenv": "^10.0.0",
    "electron": "^14.0.0",
    "electron-builder": "^22.11.7",
    "electron-notarize": "^1.1.1",
    "html-webpack-plugin": "^5.3.2",
    "ifdef-loader": "^2.3.0",
    "mini-css-extract-plugin": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.39.2",
    "sass-loader": "^12.1.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "electron-reload": "^1.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.0",
    "wait-on": "^6.0.0"
  },
  "resolutions": {
    "electron-builder/dmg-builder/dmg-license/plist/xmldom": "github:xmldom/xmldom"
  }
}
