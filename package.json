{
  "name": "elephicon",
  "productName": "Elephicon",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "version": "2.4.1",
  "license": "MIT",
  "author": {
    "name": "sprout2000"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "package": "cross-env NODE_ENV=\"development\" ts-node scripts/builder.ts",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/main.js ./dist/index.html && electronmon .",
    "release": "ts-node scripts/builder.ts"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "i18next": "^22.4.15",
    "localforage": "^1.10.0",
    "mime-types": "^2.1.35",
    "png2icons": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.15.12",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "electron": "^24.1.2",
    "electron-builder": "^24.2.1",
    "electronmon": "^2.0.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "wait-on": "^7.0.1",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.1"
  }
}
