{
  "name": "elephicon",
  "productName": "Elephicon",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "version": "3.0.5",
  "license": "MIT",
  "author": {
    "name": "sprout2000"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/index.html ./dist/main.js && electronmon .",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "lint": "biome check --write .",
    "package": "ts-node scripts/builder.ts",
    "release": "ts-node scripts/builder.ts"
  },
  "dependencies": {
    "electron-log": "^5.2.0",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.9",
    "i18next": "^23.15.2",
    "localforage": "^1.10.0",
    "mime-types": "^2.1.35",
    "png2icons": "^2.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.16.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "electron": "^29.4.6",
    "electron-builder": "^24.13.3",
    "electronmon": "^2.0.3",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.79.4",
    "sass-loader": "^14.2.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "wait-on": "^8.0.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
