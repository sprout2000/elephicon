{
  "name": "elephicon",
  "productName": "Elephicon",
  "version": "0.13.0",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "author": "sprout2000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/elephicon/issues"
  },
  "keywords": [
    "electron",
    "ico",
    "icns",
    "png2icons"
  ],
  "engines": {
    "node": ">=14",
    "npm": "please_use_yarn_instead",
    "yarn": ">=1.22"
  },
  "os": [
    "win32",
    "darwin",
    "linux"
  ],
  "main": "dist/main.js",
  "scripts": {
    "start": "run-p devtools build",
    "build": "run-s clean build:dev serve",
    "serve": "electron .",
    "devtools": "react-devtools",
    "clean": "rimraf dist release",
    "build:dev": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build:prod": "cross-env NODE_ENV=\"production\" webpack",
    "release": "run-s build:prod package:sign",
    "package": "run-s build:prod package:unsign",
    "package:appx": "run-s build:prod package:sign",
    "package:sign": "ts-node scripts/build.ts",
    "package:unsign": "ts-node scripts/build-unsign.ts"
  },
  "dependencies": {
    "electron-log": "^4.3.1",
    "electron-store": "^6.0.0",
    "electron-updater": "^4.3.5",
    "i18next": "^19.8.4",
    "mime-types": "^2.1.27",
    "png2icons": "^2.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "typeface-roboto": "^1.1.13",
    "ua-parser-js": "^0.7.23"
  },
  "devDependencies": {
    "@types/css-minimizer-webpack-plugin": "^1.1.1",
    "@types/mime-types": "^2.1.0",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/ua-parser-js": "^0.7.35",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "dotenv": "^8.2.0",
    "electron": "^11.1.0",
    "electron-builder": "^22.8.1",
    "electron-notarize": "^1.0.0",
    "html-webpack-plugin": "^5.0.0-alpha.15",
    "mini-css-extract-plugin": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.2.1",
    "react-devtools": "^4.10.1"
  }
}
