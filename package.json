{
  "name": "elephicon",
  "productName": "Elephicon",
  "version": "0.17.5",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "author": "sprout2000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/elephicon/issues"
  },
  "keywords": [
    "electron",
    "ico",
    "icns",
    "png2icons"
  ],
  "engines": {
    "node": ">=14.0.0 <16.0.0",
    "npm": "Use yarn instead and take care of yarn.lock."
  },
  "main": "dist/main.js",
  "scripts": {
    "start": "run-s clean build serve",
    "serve": "electron .",
    "clean": "rimraf dist release",
    "clean:prod": "rimraf node_modules dist release",
    "build": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build:prod": "cross-env NODE_ENV=\"production\" webpack --progress",
    "build:unsign": "ts-node scripts/build-unsign.ts",
    "build:darwin": "ts-node scripts/build.ts",
    "build:appx": "ts-node scripts/build-appx.ts",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx --fix",
    "lint:prettier": "prettier --write .",
    "ci": "yarn install --ignore-optional --frozen-lockfile",
    "package": "run-s clean:prod ci build:prod build:unsign",
    "package:appx": "run-s clean:prod ci build:prod build:appx",
    "package:darwin": "run-s clean:prod ci build:prod build:darwin"
  },
  "dependencies": {
    "electron-log": "^4.3.5",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.8",
    "i18next": "^20.2.2",
    "mime-types": "^2.1.30",
    "png2icons": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "typeface-roboto": "^1.1.13",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.0",
    "@types/mini-css-extract-plugin": "^1.4.2",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/ua-parser-js": "^0.7.35",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "dotenv": "^8.2.0",
    "electron": "^12.0.6",
    "electron-builder": "22.11.1",
    "electron-notarize": "^1.0.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "ts-loader": "^9.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  }
}
